$cmds{request}{result}={serial => 1234};
$cmds{show}{cmd}='cert_show 1234 out=/tmp/quattor_nss-XXXX/init_nss_nick1.crt,version.*';
$cmds{show}{result}={serial => 1234, dn => 'mydn'};
