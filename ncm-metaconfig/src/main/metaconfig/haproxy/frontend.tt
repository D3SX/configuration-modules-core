bind [% fte.bind %][%- IF fte.port -%]:[% fte.port %][%- END %][% INCLUDE 'metaconfig/haproxy/params.tt' params=fte.params %]
[% IF fte.mode -%]
mode [% fte.mode %]
[% END -%]
[% IF fte.acl.defined -%]
[% FOREACH aclline IN fte.acl.pairs -%]
acl [% aclline.key %] [% aclline.value %]
[% END -%]
[% END -%]
[% IF fte.item('tcp-request').defined -%]
[% FOREACH tcpr IN fte.item('tcp-request') -%]
tcp-request [% tcpr %]
[% END -%]
[% END -%]
default_backend [% fte.default_backend %]
