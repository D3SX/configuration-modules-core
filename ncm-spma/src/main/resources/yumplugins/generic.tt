[[% section %]]
[% FOREACH pair IN CCM.contents.pairs -%]
[%      pair.key %]=
[%-     SWITCH pair.key -%]
[%-         CASE boolean_true_false -%]
[%-             pair.value ? 'true' : 'false' -%]
[%-         CASE -%]
[%-             CCM.is_list(pair.value) ? pair.value.join(listsep) : pair.value -%]
[%-     END %]
[% END -%]
